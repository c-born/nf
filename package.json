{
	"name": "nf",
	"displayName": "nf",
	"description": "",
	"version": "0.0.1",
	"publisher": "cborn",
	"engines": {
		"vscode": "^1.33.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:nf.helloWorld",
		"onCommand:nf.workspaceFolderFS",
		"onCommand:nf.buildDirectory",
		"onCommand:nf.nfRoot"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "nf.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "nf.workspaceFolderFS",
				"title": "workspaceFolderFS"
			},
			{
				"command": "nf.binaryDir",
				"title": "binaryDir"
			},
			{
				"command": "nf.nfRoot",
				"title": "nfRoot"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	}
}
